    }
  ],
  "scripts": {
    "test": "if [ \"${TRAVIS}\" ]; then npm run test-ci; else npm run test-local; fi",
    "test-local": "tape \"./test/*.js\" | tap-spec",
    "test-ci": "npm run test-local && xvfb-run npm run test-browsers",
    "test-cov": "istanbul cover --dir ./reports/coverage --report lcov tape -- \"./test/*.js\"",
    "test-browsers": "browserify ./test/*.js | testling | tap-spec",
    "coverage": "istanbul cover --dir ./reports/codecov/coverage --report lcovonly tape -- \"./test/*.js\" && cat ./reports/codecov/coverage/lcov.info | codecov && rm -rf ./reports/codecov"
  },
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "TODO"
  },
  "keywords": [
    "blas",
    "linear",
    "algebra",
    "math",
    "mathematics",
    "numeric",
    "compute",
    "compute.io",
    "computation",
    "matrix",
    "vector",
    "subroutines"
  ],
  "bugs": {
    "url": "TODO"
  },
  "dependencies": {
    "blas-dasum": "^1.0.1",
    "blas-daxpy": "^1.0.3",
    "blas-dcopy": "^1.0.1",
    "blas-ddot": "^1.0.0",
    "blas-dscal": "^1.0.0",
    "blas-dswap": "^1.0.0"
  },
  "devDependencies": {
    "browserify": "12.x.x",
    "codecov": "1.x.x",
    "istanbul": "^0.4.1",
    "jshint": "2.x.x",
    "jshint-stylish": "2.x.x",
    "object-keys": "^1.0.9",
    "tap-spec": "4.x.x",
    "tape": "4.x.x",
    "testling": "1.x.x"
  },
  "testling": {
    "files": [
      "test/*.js"
    ],
    "browsers": [
      "iexplore/6.0..latest",
      "firefox/3.0..latest",
      "firefox/nightly",
      "chrome/4.0..latest",
      "chrome/canary",
